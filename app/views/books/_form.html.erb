<%= form_for @book do |f| %>
<%= f.label :title, "Enter title" %>
<%= f.text_field :title %>
<%= f.label :author, "Enter author" %>
<%= f.text_field :author %>
<%= f.label :rating, "Enter rating" %>
<%= f.text_field :rating %>
<%= f.label "I have read this book" %>
<%= f.check_box :have_read %> <br />
<%= f.submit %>
<% end %>
